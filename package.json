{
  "name": "kr-api",
  "version": "5.7.0",
  "private": true,
  "description": "Klickrhein Feature API (Freepy Sapling)",
  "main": "dist/index.js",
  "scripts": {
    "reset-dist": "node .develop/reset-dist.js",
    "develop": "npm run reset-dist && concurrently -n 'TSC,API' -c 'auto' 'tsc -w' 'nodemon dist/index.js --delay 250ms'",
    "watch": "npm run reset-dist && tsc -w",
    "build": "npm run reset-dist && tsc",
    "start": "node dist/index.js",
    "format": "prettier --ignore-path .gitignore --write .",
    "update": "ncu"
  },
  "type": "module",
  "author": "Felix Heidecke",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/session": "^10.7.2",
    "@fastify/swagger": "^8.14.0",
    "@noble/hashes": "^1.4.0",
    "@sentry/node": "^7.112.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "knex": "^3.1.0",
    "lodash-es": "^4.17.21",
    "mysql2": "^3.9.7",
    "nodemailer": "^6.9.13",
    "redis": "^4.6.13",
    "slashes": "^3.0.12",
    "slugify": "^1.6.6",
    "textile-js": "^2.1.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "concurrently": "^8.2.2",
    "eslint": "^9.1.1",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.0",
    "npm-check-updates": "^16.14.18",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  }
}
